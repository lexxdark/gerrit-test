[project]
	description = Testing gerrit with gerrit hub
	state = hidden
[access "refs/*"]
	owner = group user/lexxdark (lexxdark)
[access "refs/for/refs/*"]
	read = group user/lexxdark (lexxdark)
	push = group user/lexxdark (lexxdark)
	label-Code-Review = -2..+2 group user/lexxdark (lexxdark)
	label-Verified = -1..+1 group user/lexxdark (lexxdark)
	rebase = group user/lexxdark (lexxdark)
	removeReviewer = group user/lexxdark (lexxdark)
	submit = group user/lexxdark (lexxdark)
[access "refs/heads/*"]
	read = group user/lexxdark (lexxdark)
	create = group user/lexxdark (lexxdark)
	push = +force group user/lexxdark (lexxdark)
	pushMerge = group user/lexxdark (lexxdark)
[access "refs/tags/*"]
	push = +force group user/lexxdark (lexxdark)
	pushTag = group user/lexxdark (lexxdark)
	pushSignedTag = group user/lexxdark (lexxdark)
[submit]
	action = rebase if necessary
